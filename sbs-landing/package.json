{
    "name": "sbs-integration-engine",
    "version": "1.0.0",
    "description": "Saudi Billing System (SBS) Integration Engine - Backend API with GIVC Health Frontend",
    "main": "server.js",
    "type": "commonjs",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "dev:frontend": "vite",
        "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
        "build": "vite build",
        "build:backend": "echo \"Backend is Node.js - no build required\"",
        "preview": "vite preview",
        "test": "node test-submit-claim.js",
        "lint": "eslint .",
        "format": "prettier --write \"**/*.{js,jsx,json,css}\""
    },
    "keywords": [
        "sbs",
        "billing",
        "nphies",
        "healthcare",
        "claim-processing",
        "n8n",
        "microservices",
        "givc",
        "react",
        "virtual-care"
    ],
    "author": "Dr. Mohamed El Fadil",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.7.9",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "helmet": "^8.0.0",
        "lucide-react": "^0.462.0",
        "multer": "^2.0.2",
        "pdfkit": "^0.16.0",
        "qs": "^6.14.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "uuid": "^11.0.5"
    },
    "devDependencies": {
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.2",
        "eslint": "^9.18.0",
        "nodemon": "^3.1.9",
        "playwright": "^1.50.0",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "tailwindcss": "^3.4.17",
        "vite": "^6.0.5"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Fadil369/sbs.git"
    },
    "bugs": {
