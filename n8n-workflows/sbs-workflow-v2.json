      "position": [460, 300]
    },
    {
      "parameters": {
        "jsCode": "const normalized = $input.item.json;\nconst rawClaim = $('Webhook: HIS Claim').item.json.body;\n\nreturn {\n  json: {\n    resourceType: \"Claim\",\n    status: \"active\",\n    facility_id: rawClaim.facility_id,\n    patient: {\n      reference: rawClaim.patient_id || \"Patient/12345\"\n    },\n    provider: {\n      reference: `Organization/${rawClaim.facility_id}`\n    },\n    item: [{\n      sequence: 1,\n      productOrService: {\n        coding: [{\n          system: \"http://sbs.sa/coding/services\",\n          code: normalized.sbs_mapped_code,\n          display: normalized.official_description\n        }]\n      }\n    }]\n  }\n};"
      },
      "id": "fhir-builder-node",
      "name": "Build FHIR",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [680, 300]
